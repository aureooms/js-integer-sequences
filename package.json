{
  "name": "@aureooms/js-integer-sequences",
  "description": "Integer sequences for JavaScript",
  "version": "0.0.1",
  "author": "aureooms",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-integer-sequences/issues"
  },
  "dependencies": {
    "@aureooms/js-error": "^4.0.0",
    "@aureooms/js-fibonacci": "^3.1.0",
    "@aureooms/js-itertools": "^3.3.0"
  },
  "devDependencies": {
    "@aureooms/js-integer": "3.0.0",
    "@aureooms/js-number": "3.1.0",
    "ava": "3.8.1",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "2.13.3",
    "esdoc": "1.1.0",
    "nyc": "15.0.1",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.1"
  },
  "homepage": "https://aureooms.github.io/js-integer-sequences",
  "keywords": [
    "computer-science",
    "integer-sequences",
    "mathematics"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-integer-sequences"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "test": "ava",
    "esdoc": "esdoc",
    "prepare": "npm run build"
  },
  "files": [
    "lib"
  ]
}
